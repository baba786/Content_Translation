{
  "name": "content-translation",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://baba786.github.io/Content_Translation/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode dev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
